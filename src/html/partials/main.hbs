<!doctype html>
<html lang="en-gb" class="template-{{page.template.name}}">
<head>
	<meta name="robots" content="noindex, nofollow">

	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="format-detection" content="telephone=no">

	<title>{{page.meta_title}}</title>

	<meta name="description" content="This page is called {{page.title}}">

	<link rel="shortcut icon" href="./favicon.ico">

	<link rel="stylesheet" href="./css/uikit-theme.min.css" />
	<link rel="stylesheet" href="./css/mmenu-theme.min.css" />
	<link rel="stylesheet" href="./css/main.min.css" />
</head>
<body>

	<div>

		<header class="header" data-uk-sticky="media: 960; animation: uk-animation-slide-top; show-on-up: true;">
			<nav class="uk-navbar-container uk-navbar uk-navbar-transparent" data-uk-navbar>

				<div class="uk-navbar-left">
					<a href="./" class="uk-navbar-item uk-logo">
						<img src="{{logo}}" alt="{{nb.siteName}} Logo" width="137" height="131" loading="lazy">
					</a>
				</div>

				<div class="uk-navbar-right">
					<div id="nav" class="uk-visible@m">
						{{> renderNavigation items=menu active=page.name}}
					</div>
					<a href="#mmenu" class="uk-navbar-toggle uk-hidden@m" aria-label="Toggle Navigation">
						<span data-uk-navbar-toggle-icon></span>
						<span class="uk-hidden" data-uk-close></span>
					</a>
				</div>

			</nav>
		</header>

		<main class="uk-margin-bottom">

			{{#if page.banner}}
				{{> banner}}
			{{else if page.styleguide}}
				{{#> ukContainer size=(default page.ukContainer ukContainer) class="uk-margin-top"}}
					{{#*inline "out"}}			
						<p class="uk-text-large">General internal page with Style guide</p>
						{{> ukBreadcrumb}}
						<h1>{{page.h1}}</h1>
					{{/inline}}
				{{/ukContainer}}					
			{{else}}
				{{#> ukContainer size=(default page.ukContainer ukContainer) class="uk-margin-top"}}
					{{#*inline "out"}}
						<h1>{{page.h1}}</h1>
						{{> ukBreadcrumb}}
					{{/inline}}
				{{/ukContainer}}
			{{/if}}

			{{#> main}}{{/main}}

		</main>

		<footer class="footer uk-padding-large uk-position-relative uk-padding-remove-horizontal">
			<div class="uk-container">
				<div class="footer-top">
					{{#> ukGrid class="uk-grid-medium uk-child-width-1-4@l uk-child-width-1-3@m uk-child-width-1-2@s uk-flex-wrap"}}
						{{#*inline "cells"}}

						<div class="footer-social">
							<a href="./" class="uk-logo uk-display-block">
								<img src="{{logo}}" alt="{{nb.siteName}} Logo" width="137" height="131" loading="lazy">
							</a>						
							{{>share footer=true}}
						</div>

						<div class="footer-contact">
							<span>Contact</span>
							<ul class="uk-list">
								<li><a href="mailto:info@thymeaccounting.co.uk">info@thymeaccounting.co.uk</a></li>
								<li><a href="tel:+4407779621967">+44 (0) 7779621967</a></li>
								<li class="uk-flex uk-flex-middle">{{> renderIcon name="whatsapp-green" class="uk-margin-small-right"}}<a class="whatsapp" href="#">Message us on WhatsApp</a></li>
							</ul>
							<ul class="uk-list">
								<li class="uk-flex uk-flex-middle"><span class="uk-display-inline-block">Aberdeen:</span><a href="tel:+4401224518825">+44 (0) 1224 518825</a></li>
								<li class="uk-flex uk-flex-middle"><span class="uk-display-inline-block">Edinburgh:</span><a href="tel:+4401313644010">+44 (0) 131 364 4010</a></li>
								<li class="uk-flex uk-flex-middle"><span class="uk-display-inline-block">Newcastle:</span><a href="tel:+4401915806131">+44 (0) 191 580 6131</a></li>
								<li class="uk-flex uk-flex-middle"><span class="uk-display-inline-block">Shetland:</span><a href="tel:+4401595922080">+44 (0) 1595 922080</a></li>
							</ul>
						</div>

						<div>
							<span>Sectors</span>
							<ul class="uk-list">
								<li><a href="#">Contractors</a></li>
								<li><a href="#">Engineering</a></li>
								<li><a href="#">Barbers</a></li>
								<li><a href="#">Technology</a></li>
								<li><a href="#">Salons</a></li>
								<li><a href="#">Hospitality</a></li>
								<li><a href="#">Charities</a></li>
								<li><a href="#">Coffee Shops</a></li>
								<li><a href="#">Start-Ups</a></li>
								<li><a href="#">SME’s</a></li>
							</ul>
						</div>

						<div>
							<span>Quick Links</span>
							<ul class="uk-list">
								<li><a href="#">Sign into Client Area </a></li>
								<li><a href="#">FAQ’s</a></li>
								<li><a href="#">Fee Calculator</a></li>
								<li><a href="#">Self-Assessment Checklist</a></li>
								<li><a href="#">Standard Engagement Letters</a></li>
							</ul>
						</div>

						{{/inline}}
					{{/ukGrid}}
				</div>
			</div>
			<div class="footer-bottom">
				<div class="uk-container uk-text-center">

					<div class="uk-flex uk-flex-between uk-flex-middle uk-flex-wrap">

						<div class="copyright uk-text-left uk-margin-right">&copy; {{year}} {{nb.clientName}}. All rights reserved.</div>

						<div class="uk-flex uk-flex-middle">
							<div class="legal">
								<ul>
								{{#each legal}}
									<li>
										<a href="#">{{this}}</a>
									</li>
								{{/each}}
								</ul>
							</div>

							<div class="watermark">
								Website by <a title="NB Communication - Digital Marketing Agency" target="_blank" rel="noopener" href="https://www.nbcommunication.com/">NB <img src="./img/nb-logo.png" alt="NB Communication Ltd Logo" loading="lazy"></a>
							</div>
						</div>
					</div>
				</div>

			</div>
			{{> renderIcon name="footer-flower" class="uk-position-bottom-right"}}
		</footer>
	</div>

	<div id="mmenu" class="uk-hidden">
		{{> renderNavigation items=menu mobile=true active=page.name}}
	</div>

	<script src="./js/uikit.min.js"></script>
	<script src="./js/nbkit.min.js"></script>
	<script src="./js/mmenu.js"></script>
	<script src="./js/main.js"></script>
</body>
</html>
