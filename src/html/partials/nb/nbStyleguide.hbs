{{#> nbForm}}
	{{#*inline "form"}}

	<form id="nb-form-{{page.name}}" class="uk-form-stacked InputfieldForm" method="post" data-uk-nb-form novalidate data-colspacing="1">

		<h2>Form Styles</h2>

		{{#> ukGrid class="uk-grid-small"}}
			{{#*inline "cells"}}

			{{> nbInput class="uk-width-2-3@m" label="Your Name" name="name" title="Please enter your name"}}

			{{> nbInput class="uk-width-1-2@m" label="Telephone" name="tel" type="tel" }}

			<div class="Inputfield uk-width-2-3@m nb-form-required uk-first-column" id="wrap_nb-form-styleguide_select">

				<label class="uk-form-label" for="nb-form-styleguide_select">Selector <span>*</span></label>

				<div class="nb-form-content">
					<div role="alert"></div>

					<div data-uk-form-custom="target: > * > span:first-child" class="uk-width-1-1">
						<select class="uk-select" id="form-stacked-select">
							<option value="">--- Select ---</option>
							<option>Option 01</option>
							<option>Option 02</option>
						</select>
						<button class="uk-flex uk-flex-middle uk-flex-between uk-width-1-1" type="button">
							<span></span>
							{{> renderIcon name="chevron-down"}}
						</button>						
					</div>

				</div>
			</div>

			{{> nbInput class="uk-width-2-3@m" label="Inactive field" name="inactive" type="text" disabled="Jane" }}

			{{> nbInput label="Your Enquiry" name="enquiry" type="textarea"}}

			<div class="uk-margin uk-grid-small uk-child-width-auto uk-grid">
				<label class="uk-text-small uk-flex uk-flex-top uk-margin-bottom"><input class="uk-checkbox" type="checkbox" > By clicking confirm you agree to our Engagement Letter, Terms and Conditions and the Privacy Notice documents.</label>
				<label class="uk-text-small uk-flex uk-flex-top"><input class="uk-checkbox" type="checkbox" checked> Confirmed state</label>
			</div>

			<div class="uk-flex uk-flex-middle">
				{{> nbInput class="uk-margin-right" name="submit" type="submit" value="Send a Message"}}
				{{> ukButton label="Cancel" style="default"}}
			</div>

			{{/inline}}
		{{/ukGrid}}

	</form>

	{{/inline}}
{{/nbForm}}

