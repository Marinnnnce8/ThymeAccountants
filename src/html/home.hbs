{{#> main}}
	{{#*inline "main"}}

	{{> homeBanner}}

	<section data-uk-scrollspy="cls: uk-animation-slide-bottom; target: .uk-card; delay: 350;" id="sectors-section" class="sectors uk-section">
		<div class="uk-container">
			{{#> ukGrid class="uk-grid-small uk-child-width-1-2@m uk-flex-wrap uk-flex-bottom uk-margin-medium-bottom"}}
				{{#*inline "cells"}}
					<div>
						<h2>No matter what business you’re in, <span class="underlined uk-text-secondary">we can help you.</span></h2>
					</div>
					<div class="uk-text-right@m">
						{{>ukButton link="#" style="text" size="large" label="View all sectors" icon="arrow-right"}}
					</div>
				{{/inline}}
			{{/ukGrid}}		
		</div>
		<div class="uk-container uk-container-large">	
			{{> nbItems items=items.children}}
		</div>
	</section>
	
	{{#> ukSection id="find-out-more-section"}}
		{{#*inline "out"}}
			<div class="uk-border-rounded find-out-more left uk-text-center uk-position-relative uk-flex uk-flex-middle uk-overlow-hidden" data-uk-parallax="opacity: 0,1; y: 100,0; viewport: 0.5">
				<a href="#" class="uk-position-cover" aria-label="Contact us"></a>
				<img src="./img/lets-talk-mob.png" width="173" height="189" alt="Let's talk" loading="lazy">
				<h2 class="uk-margin-remove">Want to find out more? <span class="underlined">Let’s talk</span>.</h2>
				{{> renderIcon name="find-out-more" viewBox="244 126" class="uk-position-bottom-center"}}
			</div>
		{{/inline}}
	{{/ukSection}}

	{{>notification id="notification-section"}}

	{{>reviews id="reviews-section"}}
		
	{{#> ukSection id="find-out-more-call-section"}}
		{{#*inline "out"}}
			<div class="uk-border-rounded find-out-more right uk-text-center uk-position-relative uk-flex uk-flex-middle uk-overlow-hidden" data-uk-parallax="opacity: 0,1; y: 100,0; viewport: 0.5">
				<a href="#" class="uk-position-cover" aria-label="Contact us"></a>
				<img src="./img/lets-talk-mob-2.png" width="173" height="189" alt="Let's talk" loading="lazy">
				<h2 class="uk-h3 uk-margin-remove">Give us a call and we can chat through your requirements. <span class="underlined">Let’s talk</span>.</h2>
				{{> renderIcon name="find-out-more" viewBox="244 126" class="uk-position-bottom-center"}}
			</div>
		{{/inline}}
	{{/ukSection}}

	{{>updates id="updates-section"}}

	{{>signupBanner}}

	{{>services items=items.services}}

	{{/inline}}
{{/main}}
